<% include partials/header %>
<style>
    h1 {
        margin: 20px;
    }

    .container {
        font-size: 125%;
    }

    #button {
        text-align: center;
    }

    input[type="checkbox"] {
        margin-right: 5px;
        margin-top: 5px;
    }

    .pding {
        padding-left: 50px;
    }

    input[type="number"] {
        width: 50px;
    }

    #submitButton {
        height: 40px;
        background: #0062ff;
        border: none;
        color: white;
        border-radius: 18px;
        font-weight: bold;
        font-size: 125%;
    }

    .notVisible {
        display: none;
    }

    #textarea {
        width: 100%;
        height: 125px;
        font-size: 2.0rem;
        text-align: center;
        padding: 45px;
    }

    #textarea:focus {
        width: 100%;
        height: 125px;
        font-size: 2.0rem;
        padding: 5px;
        text-align: left;
    }

    .form-group {
        padding: 5px;
        padding-bottom: 3px;
        border: 1px solid gray;
        border-radius: 1.0rem;
        text-align: center;
    }

    .msg {
        font-size: 120%;
    }

    body {
        color: white
    }

    .card {
        border-radius: 20px;
    }

    .card:hover {
        border-radius: 20px;
        box-shadow: 10px 10px 7px #D30755;
        transition: all 200ms ease-out;
    }
</style>
<div class="section lb">
    <div class="container">
        <%=user.patientname%> and  <%=user.doctorname%>
    </div>
</div>
</div>
<% include partials/footer %>